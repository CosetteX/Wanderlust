<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wanderlust">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.example.wanderlust.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.wanderlust.Bar_1.Bar_1_Display" />
        <activity android:name="com.example.wanderlust.Bar_1.Bar_1_Color" />
        <activity android:name="com.example.wanderlust.Bar_1.Bar_1_Flag" />
        <activity
            android:name=".activity.InsertAttendanceInfoActivity"
            android:screenOrientation="behind" />
        <activity android:name=".activity.ScheduleDetailActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>